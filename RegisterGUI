    private void btn_finishActionPerformed(java.awt.event.ActionEvent evt) {
        // TODO add your handling code here:
        // Add code to parse json file for username and password matches...

        //if statement here...
        Response.show();
        Register.hide();

        new java.util.Timer().schedule(new TimerTask() {
            @Override
            public void run() {
                Register.show();
                Response.hide();
            }
        }, 1000 * 5);
    }

